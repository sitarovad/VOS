<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
      <p>Email: <%= @user.login%>
      <div>Signed up: <%= @user.created_at%></p>
    </section>
  </aside>
</div>